{
  "name": "app.ui.admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "generate-client": "nswag swagger2tsclient /input:%npm_package_devConfig_genClient_input% /output:%npm_package_devConfig_genClient_output%"
  },
  "devConfig": {
    "genClient": {
      "input": "http://localhost:56833/swagger/v1/swagger.json",
      "output": "src/api-client/client.ts"
    }
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "ant-design-vue": "^1.6.2",
    "mavon-editor": "^2.9.0",
    "vue": "^2.6.11",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.3.2",
    "vuex": "^3.4.0",
    "vuex-class": "^0.3.2",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@vue/cli-plugin-babel": "^4.4.1",
    "@vue/cli-plugin-eslint": "~4.4.1",
    "@vue/cli-plugin-typescript": "^4.4.1",
    "@vue/cli-service": "^4.4.1",
    "@vue/eslint-config-typescript": "^5.0.2",
    "copy-webpack-plugin": "^6.0.2",
    "eslint": "^7.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^3.11.2",
    "less-loader": "^6.1.0",
    "lint-staged": "^10.2.8",
    "nswag": "^13.6.0",
    "typescript": "^3.9.3",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
