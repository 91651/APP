{
  "name": "app.ui.admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "generate-client": "nswag swagger2tsclient /ClientBaseClass:BaseClient /ExtensionCode:%npm_package_devConfig_genClient_extCode% /UseTransformOptionsMethod:true /input:%npm_package_devConfig_genClient_input% /output:%npm_package_devConfig_genClient_output%"
  },
  "devConfig": {
    "genClient": {
      "input": "http://localhost:56833/swagger/v1/swagger.json",
      "output": "src/api-client/client.ts",
      "extCode": "src/api-client/base.ts"
    }
  },
  "dependencies": {
    "iview": "^3.1.5",
    "mavon-editor": "^2.6.17",
    "vue": "^2.5.21",
    "vue-property-decorator": "^7.2.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-typescript": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "copy-webpack-plugin": "^4.6.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "nswag": "^12.0.7",
    "typescript": "^3.2.2",
    "vue-template-compiler": "^2.5.21"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
